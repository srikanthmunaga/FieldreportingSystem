		<parameter name="emp_id" class="java.lang.String"/>
		<parameter name="fdate" class="java.lang.String"/>
		<parameter name="tdate" class="java.lang.String"/>
		<queryString>
			<![CDATA[select 
unitcode,
unit_name,
area_name,
H0_REMARKS,HO_REASON,
WAR_POD,
sum(FRS_OD_AMT) as SFRS_OD_AMT,
sum(FRS_VILLAGE_COUNT) as SFRS_VILLAGE_COUNT,
sum(FRS_CUST_COUNT) as SFRS_CUST_COUNT,
sum(WAR_VILLAGES) as SWAR_VILLAGE_COUNT,
sum(WAR_CUST_COUNT) as SWAR_CUST_COUNT,
sum(WAR_SDRCUST_COUNT) as SWAR_SDRCUST_COUNT,
sum(FRS_SDRCUST_COUNT) as SFRS_SDRCUST_COUNT,
sum(TARGET_FRS_OD_AMT) as STARGET_FRS_OD_AMT 
from   
(select
to_char(h.dates,'dd-Mon-yyyy') as FRS_DATE,
to_char(h.dates,'Day') as Days,
h.huma_id,
(select Actual_pod(TO_DATE($P{fdate},'dd-mm-yyyy'),'HO','') from dual) as WAR_POD,
(select HUMA_FNAME||' '||HUMA_LNAME from huma_mstr where huma_id=h.huma_id) as HUMA_NAME,
(select bsflunit_ucode from huma_mstr where huma_id=h.huma_id) as unitcode,
(select area_name from area_mstr where area_id=(select area_id from bsflunit_mstr where bsflunit_ucode=(select bsflunit_ucode from huma_mstr where huma_id=h.huma_id))) as area_name,
(select bsflunit_name from bsflunit_mstr where bsflunit_ucode=(select bsflunit_ucode from huma_mstr where huma_id=h.huma_id)) as unit_name,
(select war_Backlogs(TO_DATE($P{tdate},'dd-mm-yyyy'),'HO',(select bsflunit_ucode from huma_mstr where huma_id=h.huma_id)) from dual) H0_REMARKS,
(select war_Reasons(TO_DATE($P{tdate},'dd-mm-yyyy'),'HO',(select bsflunit_ucode from huma_mstr where huma_id=h.huma_id)) from dual) as HO_REASON,
h.huma_id HUMA_ID,
fr.FRS_OD_AMT FRS_OD_AMT ,
fr.FRS_VILLAGE_COUNT FRS_VILLAGE_COUNT,
fr.FRS_CUST_COUNT FRS_CUST_COUNT,
length(wt.WAR_VILLAGES)-length(replace(wt.WAR_VILLAGES,'::',','))+1 as WAR_VILLAGES,  
wt.WAR_CUST_COUNT WAR_CUST_COUNT,
wt.WAR_SDRCUST_COUNT WAR_SDRCUST_COUNT, 
fr.FRS_SDRCUST_COUNT FRS_SDRCUST_COUNT,
tfr.TARGET_FRS_OD_AMT TARGET_FRS_OD_AMT 
FROM (SELECT *
            FROM (    SELECT TO_DATE ($P{fdate}, 'dd-mm-yyyy') + LEVEL - 1 dates
                        FROM DUAL
                  CONNECT BY LEVEL <=
                                  (  TO_DATE ($P{tdate}, 'dd-mm-yyyy')
                                   - TO_DATE ($P{fdate}, 'dd-mm-yyyy'))
                                + 1) d,
                 (SELECT huma_id
                    FROM huma_mstr
                   WHERE bsflunit_ucode IN ($P!{emp_id}) and upper(huma_status) LIKE '%ACTIVE%' and huma_DESIGNATION IN (select grade_id from grade_mstr where (GRADE_name like '%LSR%' or GRADE_name like '%FX%' or GRADE_name like '%FS%'))) h) h,
         WAR_TARGET wt,
         TARGET_FRS_RECOVERY tfr,
         FRS_RECOVERY fr
   WHERE     (    h.dates = wt.war_date(+)
              AND h.dates = tfr.target_frs_date(+)
              AND h.dates = fr.frs_date(+))
         AND (    h.huma_id = wt.huma_id(+)
              AND h.huma_id = tfr.huma_id(+)
              AND h.huma_id = fr.huma_id(+))
order by area_name,unit_name,h.HUMA_ID,h.dates) 
group by unitcode ,unit_name,area_name,H0_REMARKS,HO_REASON,WAR_POD]]>
		</queryString>
