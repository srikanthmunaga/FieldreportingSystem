		<queryString>
			<![CDATA[select
area_name,
AH_REMARKS,
AO_REASON,
WAR_POD,
sum(FRS_OD_AMT) FRS_OD_AMT,
sum(FRS_VILLAGE_COUNT) FRS_VILLAGE_COUNT,
sum(FRS_CUST_COUNT) FRS_CUST_COUNT,
sum(WAR_CUST_COUNT) WAR_CUST_COUNT,
sum(WAR_SDRCUST_COUNT) WAR_SDRCUST_COUNT,
sum(FRS_SDRCUST_COUNT) FRS_SDRCUST_COUNT,
sum(TARGET_FRS_OD_AMT) TARGET_FRS_OD_AMT,
sum(WAR_VILLAGES) as WAR_VILLAGES 
from
(select
(select area_name from area_mstr where area_id=(select area_id from bsflunit_mstr where bsflunit_ucode=(select bsflunit_ucode from huma_mstr where huma_id=h.huma_id))) as area_name,
(select war_Backlogs(TO_DATE($P{tdate},'dd-mm-yyyy'),'AH',(select area_id from bsflunit_mstr where bsflunit_ucode in (select bsflunit_ucode from huma_mstr where huma_id=h.huma_id))) from dual) as AH_REMARKS,
(select war_Reasons(TO_DATE($P{tdate},'dd-mm-yyyy'),'AH',(select area_id from bsflunit_mstr where bsflunit_ucode in (select bsflunit_ucode from huma_mstr where huma_id=h.huma_id))) from dual) as  AO_REASON,
fr.FRS_OD_AMT FRS_OD_AMT,
(select Actual_pod(TO_DATE($P{fdate},'dd-mm-yyyy'),'AH',(select area_id from bsflunit_mstr where bsflunit_ucode in (select bsflunit_ucode from huma_mstr where huma_id=h.huma_id))) from dual) as WAR_POD,
fr.FRS_VILLAGE_COUNT FRS_VILLAGE_COUNT,
fr.FRS_CUST_COUNT FRS_CUST_COUNT,
wt.WAR_CUST_COUNT WAR_CUST_COUNT,
wt.WAR_SDRCUST_COUNT WAR_SDRCUST_COUNT,
fr.FRS_SDRCUST_COUNT FRS_SDRCUST_COUNT,
tfr.TARGET_FRS_OD_AMT TARGET_FRS_OD_AMT,
length(wt.WAR_VILLAGES)-length(replace(wt.WAR_VILLAGES,'::',','))+1 as WAR_VILLAGES 
FROM (SELECT *
            FROM (    SELECT TO_DATE ($P{fdate}, 'dd-mm-yyyy') + LEVEL - 1 dates
                        FROM DUAL
                  CONNECT BY LEVEL <=
                                  (  TO_DATE ($P{tdate}, 'dd-mm-yyyy')
                                   - TO_DATE ($P{fdate}, 'dd-mm-yyyy'))
                                + 1) d,
                 (SELECT huma_id
                    FROM huma_mstr
                   WHERE bsflunit_ucode IN (select bsflunit_ucode from bsflunit_mstr where area_id in($P!{emp_id})) and upper(huma_status) LIKE '%ACTIVE%' and huma_DESIGNATION IN (select grade_id from grade_mstr where (GRADE_name like '%LSR%' or GRADE_name like '%FX%' or GRADE_name like '%FS%'))) h) h,
         WAR_TARGET wt,
         TARGET_FRS_RECOVERY tfr,
         FRS_RECOVERY fr
   WHERE     (    h.dates = wt.war_date(+)
              AND h.dates = tfr.target_frs_date(+)
              AND h.dates = fr.frs_date(+))
         AND (    h.huma_id = wt.huma_id(+)
              AND h.huma_id = tfr.huma_id(+)
              AND h.huma_id = fr.huma_id(+))) 
group by area_name,AH_REMARKS,AO_REASON,WAR_POD 
order by area_name]]>
		</queryString>
